    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: fastapi",
            "type": "debugpy",
            "request": "launch",
            "console": "integratedTerminal",
            "justMyCode": false,
            "module": "uvicorn",
            "args": ["app.main:app","--reload"]
        },
        {
            "name": "Python: Current File",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
          }
    ]
}